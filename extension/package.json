{
	"name": "extension",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/chrome": "^0.0.177",
		"@types/jest": "^27.4.0",
		"@types/node": "^16.11.19",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"axios": "^0.25.0",
		"bootstrap": "^5.1.3",
		"cross-env": "^5.0.1",
		"prop-types": "^15.8.1",
		"react": "^17.0.2",
		"react-bootstrap": "^2.1.1",
		"react-dom": "^17.0.2",
		"react-hook-form": "^7.27.0",
		"react-router-dom": "^6.2.1",
		"react-scripts": "5.0.0",
		"typescript": "^4.5.4",
		"web-vitals": "^2.1.3"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
		"test": "react-scripts test --coverage",
		"test-nc": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint --ext .js,.ts,.tsx .",
		"lint-fix": "eslint --fix --ext .js,.ts,.tsx  .",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|tsx|json)\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{json,js,jsx,ts,tsx,css,scss,md}": "prettier --config .prettierrc --write",
		"*.{ts,tsx,js,jsx}": "eslint --cache --max-warnings 0"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-react": "^7.28.0",
		"husky": "^4.3.8",
		"lint-staged": "^12.3.1",
		"prettier": "^2.5.1"
	}
}
